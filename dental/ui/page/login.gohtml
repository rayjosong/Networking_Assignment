{{define "title"}}Login{{end}}
{{define "main"}}
  <h1>Please login to your account</h1>
  <form method="post">
    <input class="my-1" type="text" name="username" placeholder="username" /><br />
    <input class="my-1" type="password" name="password" placeholder="password" /><br />
    <input class="my-1 px-1" type="submit" />
  </form>
  <h3>Or <a href="/signup">Sign Up</a> if you do not have an account</h3>
{{end}}
